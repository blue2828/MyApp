<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="vo.Cource" table="cource" catalog="test">
        <id name="courceId" type="java.lang.Integer">
            <column name="courceId" not-null="true"/>
            <generator class="native"/>
        </id>
        <property name="courceName" type="java.lang.String">
            <column name="courceName"/>
        </property>
    </class>
</hibernate-mapping>